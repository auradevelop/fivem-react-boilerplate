{
  "name": "react-fivem",
  "version": "1.0.0",
  "homepage": "html",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "craco start",
    "start:game": "cross-env IN_GAME_DEV=1 craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "build:clean": "rimraf ./build",
    "lint": "eslint src/**/**.js",
    "lint:fix": "eslint src/**/**.js --fix",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write **/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "keywords": [
    "fivem",
    "react"
  ],
  "dependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "@types/webpack": "^5.28.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.11.0",
    "eslint-webpack-plugin": "^3.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "craco": "^0.0.3",
    "prettier": "^1.17.1",
    "cross-env": "^7.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
